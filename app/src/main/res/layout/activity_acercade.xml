<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="?android:attr/textColorLinkInverse"
    android:padding="30dp"
    tools:context=".Acercade">

    <LinearLayout
        android:layout_width="409dp"
        android:layout_height="700dp"
        android:orientation="vertical"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="1dp">

        <TextView
            android:id="@+id/textView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="elm350566@gmail.com"
            android:textColor="#1835D6"
            android:textSize="24sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="meg73745@gmail.com"
            android:textColor="#1835D6"
            android:textSize="24sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <MultiAutoCompleteTextView
            android:id="@+id/multiAutoCompleteTextView"
            android:layout_width="358dp"
            android:layout_height="223dp"
            android:background="@android:color/holo_orange_light"
            android:gravity="center"
            android:padding="9dp"
            android:text="ValueDuct, Busca ser un proyecto de enseñanza a niños para que aprendan de una manera interactiva y agradable para ellos, que sea motivadora y no se distraigan"
            android:textAlignment="center"
            android:textStyle="bold" />

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="match_parent"
            android:layout_height="159dp"
            app:srcCompat="@android:drawable/ic_menu_myplaces" />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>